{
    "name": "Link Router",
    "version": "0.2",
    "manifest_version": 2,
    "description": "Route links on any given page to the main log with a certain tag",

    "background": {
	"scripts": [ "moment.js", "router.js"]
    },

    "options_page": "options.html",

    "permissions": [
	"contextMenus"
    ],
    
    "icons": {
	"16": "icon-32.png",
	"48": "icon-48.png",
	"128": "icon-128.png"
    }
    
}
